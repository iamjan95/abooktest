MAIN = address_book_main
FUNCTIONS = address_book
LLFUNCTIONS = llist
STATICTEST = statictest
COMPLEXITY1= complexity1
COMPLEXITY2= complexity2
COMPLEXITY3= complexity3
COMPLEXITY4= complexity4
COMPLEXITY5= complexity5
COMPLEXITY6= complexity6
COMPLEXITY7= complexity7
COMPLEXITY8= complexity8
PATTERN= pattern1


COMPILER = g++
COMPILERFLAGS = -c -s
DEBUG = -DDEBUG

all: $(MAIN)

debug: $(MAIN)_debug

$(MAIN): $(FUNCTIONS).o $(LLFUNCTIONS).o $(MAIN).o $(STATICTEST).o $(COMPLEXITY1).o $(COMPLEXITY2).o $(COMPLEXITY3).o $(COMPLEXITY4).o $(COMPLEXITY5).o $(COMPLEXITY6).o $(COMPLEXITY7).o $(COMPLEXITY8).o $(PATTERN).o  
	$(COMPILER) $(FUNCTIONS).o $(LLFUNCTIONS).o $(MAIN).o $(STATICTEST).o $(COMPLEXITY1).o $(COMPLEXITY2).o $(COMPLEXITY3).o $(COMPLEXITY4).o $(COMPLEXITY5).o $(COMPLEXITY6).o $(COMPLEXITY7).o $(COMPLEXITY8).o $(PATTERN).o   -o cpp_address_book -lpthread

$(MAIN).o: $(MAIN).cpp
	$(COMPILER) $(COMPILERFLAGS) $(MAIN).cpp

$(STATICTEST).o: $(STATICTEST).cpp
	$(COMPILER) $(COMPILERFLAGS) $(STATICTEST).cpp

$(COMPLEXITY1).o: $(COMPLEXITY1).cpp
	$(COMPILER) $(COMPILERFLAGS) $(COMPLEXITY1).cpp

$(COMPLEXITY2).o: $(COMPLEXITY2).cpp
	$(COMPILER) $(COMPILERFLAGS) $(COMPLEXITY2).cpp

$(COMPLEXITY3).o: $(COMPLEXITY3).cpp
	$(COMPILER) $(COMPILERFLAGS) $(COMPLEXITY3).cpp

$(COMPLEXITY4).o: $(COMPLEXITY4).cpp
	$(COMPILER) $(COMPILERFLAGS) $(COMPLEXITY4).cpp

$(COMPLEXITY5).o: $(COMPLEXITY5).cpp
	$(COMPILER) $(COMPILERFLAGS) $(COMPLEXITY5).cpp

$(COMPLEXITY6).o: $(COMPLEXITY6).cpp
	$(COMPILER) $(COMPILERFLAGS) $(COMPLEXITY6).cpp

$(COMPLEXITY7).o: $(COMPLEXITY7).cpp
	$(COMPILER) $(COMPILERFLAGS) $(COMPLEXITY7).cpp

$(COMPLEXITY8).o: $(COMPLEXITY8).cpp
	$(COMPILER) $(COMPILERFLAGS) $(COMPLEXITY8).cpp

$(PATTERN).o: $(PATTERN).cpp
	$(COMPILER) $(COMPILERFLAGS) $(PATTERN).cpp 

$(FUNCTIONS).o: $(FUNCTIONS).cpp $(FUNCTIONS).h
	$(COMPILER) $(COMPILERFLAGS) $(FUNCTIONS).cpp

$(LLFUNCTIONS).o: $(LLFUNCTIONS).cpp $(LLFUNCTIONS).h record.h
	$(COMPILER) $(COMPILERFLAGS) $(LLFUNCTIONS).cpp

$(MAIN)_debug: $(FUNCTIONS)_debug.o $(LLFUNCTIONS)_debug.o $(MAIN)_debug.o $(STATICTEST)_debug.o $(COMPLEXITY1)_debug.o $(COMPLEXITY2)_debug.o $(COMPLEXITY3)_debug.o $(COMPLEXITY4)_debug.o $(COMPLEXITY5)_debug.o $(COMPLEXITY6)_debug.o $(COMPLEXITY7)_debug.o $(COMPLEXITY8)_debug.o $(PATTERN)_debug.o
	$(COMPILER) $(FUNCTIONS)_debug.o $(LLFUNCTIONS)_debug.o $(MAIN)_debug.o  $(STATICTEST)_debug.o $(COMPLEXITY1)_debug.o $(COMPLEXITY2)_debug.o $(COMPLEXITY3)_debug.o $(COMPLEXITY4)_debug.o $(COMPLEXITY5)_debug.o $(COMPLEXITY6)_debug.o $(COMPLEXITY7)_debug.o $(COMPLEXITY8)_debug.o $(PATTERN)_debug.o -o cpp_address_book_debug

$(MAIN)_debug.o: $(MAIN).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(MAIN).cpp -o $(MAIN)_debug.o

$(STATICTEST)_debug.o: $(STATICTEST).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(STATICTEST).cpp -o $(STATICTEST)_debug.o

$(COMPLEXITY1)_debug.o: $(COMPLEXITY1).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(COMPLEXITY1).cpp -o $(COMPLEXITY1)_debug.o

$(COMPLEXITY2)_debug.o: $(COMPLEXITY2).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(COMPLEXITY2).cpp -o $(COMPLEXITY2)_debug.o

$(COMPLEXITY3)_debug.o: $(COMPLEXITY3).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(COMPLEXITY3).cpp -o $(COMPLEXITY3)_debug.o

$(COMPLEXITY4)_debug.o: $(COMPLEXITY4).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(COMPLEXITY4).cpp -o $(COMPLEXITY4)_debug.o

$(COMPLEXITY5)_debug.o: $(COMPLEXITY5).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(COMPLEXITY5).cpp -o $(COMPLEXITY5)_debug.o

$(COMPLEXITY6)_debug.o: $(COMPLEXITY6).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(COMPLEXITY6).cpp -o $(COMPLEXITY6)_debug.o

$(COMPLEXITY7)_debug.o: $(COMPLEXITY7).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(COMPLEXITY7).cpp -o $(COMPLEXITY7)_debug.o

$(COMPLEXITY8)_debug.o: $(COMPLEXITY8).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(COMPLEXITY8).cpp -o $(COMPLEXITY8)_debug.o

$(PATTERN)_debug.o: $(PATTERN).cpp
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(PATTERN).cpp -o $(PATTERN)_debug.o -pthread

$(FUNCTIONS)_debug.o: $(FUNCTIONS).cpp $(FUNCTIONS).h
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(FUNCTIONS).cpp -o $(FUNCTIONS)_debug.o

$(LLFUNCTIONS)_debug.o: $(LLFUNCTIONS).cpp $(LLFUNCTIONS).h record.h
	$(COMPILER) $(COMPILERFLAGS) $(DEBUG) $(LLFUNCTIONS).cpp -o $(LLFUNCTIONS)_debug.o

clean:
	rm -f *.o cpp_address_book*
